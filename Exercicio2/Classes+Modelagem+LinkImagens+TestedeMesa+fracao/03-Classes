public class LavaRoupas {
    private String marca;
    private String modelo;
    private int kg;
    public LavaRoupas(String marca, String modelo, int kg) {
        this.marca = marca;
        this.modelo = modelo;
        this.kg = kg;
    }
    public LavaRoupas() {
		// TODO Auto-generated constructor stub
	}
	public static void LavarRoupa() {
        System.out.println("A máquina está lavando a roupa");
    }
    public void CentrifugarRoupa() {
        System.out.println("A máquina está Centrifugando a roupa");
    }
    public static void DesligarMaquina(){
        System.out.println("Máquina Desligada");
    }

