public class Cafeteira {
    private String marca;
    private String modelo;
    private String cor;
    public Cafeteira(String marca, String modelo, String cor) {
        this.marca = marca;
        this.modelo = modelo;
        this.cor = cor;
    }
    public Cafeteira() {
		// TODO Auto-generated constructor stub
	}
	public static void LigarCafeteira(){
        System.out.println("Cafeteira Ligada");
    }
    public void DesligarCafeteira(){
        System.out.println("Cafeteira Desligada");
    }
    public static void PassarCafe(){
        System.out.println("Café pronto");
    }
}
