        this.marca = marca;
        this.modelo = modelo;
    }
    public Telefone() {
		// TODO Auto-generated constructor stub
	}
	public static void Discar(){
        System.out.println("discando...");
    }
    public void Desligar(){
        System.out.println("ligação Encerrada...");
    }
    public static void Atender(){
        System.out.println("Alô...");
    }

}
