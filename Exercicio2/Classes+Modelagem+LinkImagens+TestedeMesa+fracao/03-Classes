package exercicios;

public class Carro {
    private String cor;
    private int ano;
    private String marca;
    
    public void setCor(String cor) {
    	this.cor=cor;
    }
    public Carro(String cor, int ano, String marca) {
         this.cor = cor;
         this.ano = ano;
         this.marca = marca;
    }

    
    public Carro() {
		// TODO Auto-generated constructor stub
	}
	public static void ligar(){
         System.out.println("Carro Ligado");
    }
    public void desligar(){
         System.out.println("Carro Desligado");
    }
    public static void Escrever(){
         System.out.println("vrumm");
    }
	public static void Acelerar() {
		// TODO Auto-generated method stub
		System.out.println("vrumm");
		
	}

}

package exercicios;

public class Caneta {
    private String cor;
    private int tamanho;
    private double escrita;

    public Caneta(String cor, int tamanho, double escrita) {
        this.cor = cor;
        this.tamanho = tamanho;
        this.escrita = escrita;
        
    }
    
    public Caneta() {
		// TODO Auto-generated constructor stub
	}

	public static void AbrirTampa(){
        System.out.println("Caneta Aberta");
    }
    public static void Escrever(){
        System.out.println("Você escreveu: Vai Corinthians!!!");
    }
    public void FecharTampa(){
        System.out.println("Caneta Fechada");
    }

}
package exercicios;

public class cachorro {
    private String nome;
    private String raça;
    private int idade;
    
    public cachorro (String nome, String raça, int idade) {
    	this.idade=idade;
    	this.nome=nome;
    	this.raça=raça;
    }
    public cachorro() {
		// TODO Auto-generated constructor stub
	}
	public void Latir(){
        System.out.println("auau");
    }
    public void Dormir(){
        System.out.println("zzzzzz");
    }
    public void Comer(){
        System.out.println("o cachorro está comendo");
    }

}
package exercicios;
public class bebida {
    private String modelo;
    private String marca;
    private int ml;

    public void setModelo(String modelo) {
        this.modelo = modelo;
    }
    public void setMarca(String marca) {
        this.marca = marca;
    }
    public void setMl(int ml) {
        this.ml = ml;
    }
    public static void AbirGarrafa(){
        System.out.println("Garrafa aberta");
    }
    public void FecharGarrafa(){
        System.out.println("Garrafa fechada");
    }
    public static void ServirBebida () {
        System.out.println("A bebida foi servida");
    }
}
