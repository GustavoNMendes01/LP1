public class bicicleta {
    private String marca;
    private String cor;
    private String tamanho;
    public bicicleta(String marca, String cor, String tamanho) {
        this.marca = marca;
        this.cor = cor;
        this.tamanho = tamanho;
    }
    public bicicleta() {
		// TODO Auto-generated constructor stub
	}
	public static void Pedalar() {
        System.out.println("A bicicleta está em movimento");
    }
    public void Freiar() {
        System.out.println("A bicicleta está parando");
    }
    public static void Buzinar() {
        System.out.println("prim prim");
    }
}
