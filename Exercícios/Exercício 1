import java.util.Scanner;

public class Exercicios{

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int idade;
		int meses;
		int dias;
		int soma;
		
		idade = sc.nextInt() * 365;
		meses = sc.nextInt() * 30;
		dias = sc.nextInt();
		
		soma = idade + meses + dias;
		
		System.out.println("os dias dessa pessoa Ã©:" + soma);
		
		sc.close();
		
		
	}
}