public class Exercicio7 {
	int valor;
	
public Exercicio7 (int valor) {
	this.valor = valor;
}
public void VerNumero() {
	if (valor > 10) {
	System.out.println("É MAIOR QUE 10");
		
	}else {
		System.out.println("NÃO È MAIOR QUE 10.");
	}
	
	
	
}

	public static void main(String[] args) {
		int valor = 9;
		
		Exercicio7 mensagem= new Exercicio7(valor);
		mensagem.VerNumero();
		
