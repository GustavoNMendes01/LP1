package Exercicio;

public class Exercicio13 {
	int valor1;
	int valor2;
	public Exercicio13(int valor1, int valor2) {
		this.valor1 = valor1;
		this.valor2 = valor2;
	}
	public void VerificarValor() {
		if (valor1 == valor2){
			System.out.println("numeros iguais");
		}else if(valor1 < valor2) {
			System.out.println(valor1);
			System.out.println(valor2);
			}else {
			System.out.println(valor2);
			System.out.println(valor1);
		
		
	}
}
	
	
	
	public static void main(String[] args) {
		int valor1 = 8;
		int valor2 = 11;
		
		Exercicio13 Verificacao = new Exercicio13(valor1, valor2);
		Verificacao.VerificarValor();
	

	}

}
